# Test: Compare a simple multidomain diffusion-reaction problem with respect
#   to a reference solution produced by stable versions of the program

import default_logging.ini
import default_time_stepping.ini

__name = test_cell

[grid]
dimension = 2
file = grids/test_cell.msh
initial_level = 0

[model]
order = 1

[model.time_stepping]
begin = 0.
end = 0.01
step = 2e-3
initial_step = 2e-3
min_step = 1e-3
max_step = 2e-3
decrease_factor = 0.9
increase_factor = 1.1

[model.compartments]
cell     = 0
nucleoid = 1

[model.writer]
file_path = {__name}

############################ Model for cell ############################
[model.cell.initial]
u = 10

[model.cell.reaction]
u = -5*u

[model.cell.reaction.jacobian]
du_du = -5

[model.cell.diffusion]
u = 1.

############################ Model for nucleoid ############################
[model.nucleoid.initial]
u = 5
v = 10

[model.nucleoid.reaction]
u = -u*v
v = -10*u

[model.nucleoid.reaction.jacobian]
du_du = -v
du_dv = -u
dv_du = -10
dv_dv = 0

[model.nucleoid.diffusion]
u = 1.
v = 1.

######################### Compare with reference vtk ######################### 
[wrapper.vtkcompare]
name = {__name}/{__name}-cell-00005 {__name}/{__name}-nucleoid-00005
reference = reference/cell-00005 reference/nucleoid-00005
extension = vtu vtu
relative = 2e-2
absolute = 1e-9
